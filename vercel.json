  {
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/acronyms/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/quickstart/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
