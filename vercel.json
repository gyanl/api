{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/acronyms/.*",
        "dest": "/server.js"
      }
      // route to serve index.html for all other routes
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }
  